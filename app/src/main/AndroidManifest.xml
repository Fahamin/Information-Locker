<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="lock.file.lockerinfo">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/iconli"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/iconli_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.PasswordReset"
            android:parentActivityName=".activity.MainActivity">

        </activity>

        <activity android:name=".activity.Registration" />

        <activity android:name=".activity.Login" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".activity.DataADD"
            android:parentActivityName=".activity.MainActivity"/>

        <activity
            android:name=".activity.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar"
            >

        </activity>
    </application>

</manifest>